LIFT_PASS_VALIDITY_ID  IDENTITY(1,1) PRIMARY KEY NOT NULL ,
LIFT_PASS_VALIDITY_NAME VARCHAR(20) NOT NULL
)

CREATE TABLE LIFT_PASSES(
LIFT_PASS_RECEIPT_ID IDENTITY(1,1) PRIMARY KEY NOT NULL ,
LIFT_PASS_TYPE_ID INT NOT NULL,
LIFT_PASS_VALIDITY_ID INT NOT NULL,
LIFT_PASS_OWNER_ID INT NOT NULL,

FOREIGN KEY (LIFT_PASS_OWNER_ID) REFERENCES CLIENTS(CLIENT_ID),
FOREIGN KEY (LIFT_PASS_TYPE_ID) REFERENCES LIFT_PASS_TYPE(LIFT_PASS_TYPE_ID),
FOREIGN KEY (LIFT_PASS_VALIDITY_ID) REFERENCES LIFT_PASS_VALIDITY(LIFT_PASS_VALIDITY_ID)
)

CREATE TABLE EMPLOYEES(
EMPLOYEE_ID IDENTITY(1,1) PRIMARY KEY NOT NULL, 
EMPLOYEE_DEPARTMENT_ID INT NOT NULL,
FNAME VARCHAR(20) NOT NULL,
SNAME VARCHAR(20) NOT NULL,
LNAME VARCHAR(20) NOT NULL,

FOREIGN KEY (EMPLOYEE_DEPARTMENT_ID) REFERENCES SKI_RESORT_DEPARTMENTS(SKI_RESORT_DEPARTMENT_ID)
)

CREATE TABLE CLIENTS(
